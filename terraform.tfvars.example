# terraform.tfvars 예시 파일
# 실제 값으로 수정하여 terraform.tfvars로 저장하세요

environment = "production"

# VPC 설정 (기존 VPC 사용)
vpc_id   = "vpc-xxxxxxxx"
vpc_cidr = "10.0.0.0/16"

# 서브넷 설정
private_subnet_ids = [
  "subnet-xxxxxxxx",  # ap-northeast-2a
  "subnet-yyyyyyyy",  # ap-northeast-2b
  "subnet-zzzzzzzz"   # ap-northeast-2c
]

public_subnet_ids = [
  "subnet-aaaaaaaa",  # ap-northeast-2a
  "subnet-bbbbbbbb",  # ap-northeast-2b
  "subnet-cccccccc"   # ap-northeast-2c
]

# 보안 설정
rabbitmq_password = "secure-rabbitmq-password-123!"

# 데이터베이스 설정
db_host     = "your-rds-endpoint.cluster-xxxxx.ap-northeast-2.rds.amazonaws.com"
db_name     = "fishing_chat"
db_user     = "postgres"
db_password = "secure-db-password-123!" 